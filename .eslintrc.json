{
  "parserOptions": {
    "sourceType": "module"
  },
  "globals": {
  },
  "extends": [
    "plugin:github/browser"
  ],
  "overrides": [
    {
      "files": ["dist/selector-set.js"],
      "env": {
        "browser": true,
        "amd": true
      },
      "rules": {
        "no-invalid-this": "off",
        "prettier/prettier": "off"
      }
    },
    {
      "files": ["test/**/*.js"],
      "env": {
        "browser": true,
        "qunit": true
      },
      "globals": {
        "SelectorSet": true,
        "ExemplarSelectorSet": true,
        "Benchmark": true,
        "d3": true
      }
    },
    {
      "files": ["test/runner.js"],
      "env": {
        "es6": true,
        "node": true
      },
      "parserOptions": {
        "ecmaVersion": 2017
      }
    }
  ]
}
